{
  "name": "jflauncher",
  "displayName": "JFLauncher",
  "version": "v0.1.7",
  "description": "Basic Launcher for Jimmu's Factory modpack",
  "main": "build/electron/main.js",
  "author": {
    "name": "Dragonir",
    "email": "dragonir44@gmail.com"
  },
  "license": "MIT",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dragonir44/JFLauncher.git"
  },
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "watch": "nodemon --watch \"electron/**/*\" -e ts --exec \"tsc -p electron && electron .\"",
    "elec:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"yarn watch\"",
    "elec:build": "yarn build && tsc -p electron && electron-builder",
    "elec:release": "yarn build && tsc -p electron && electron-builder build --publish=always"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@electron/packager": "^18.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-html-parser": "^2.0.6",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "electron-winstaller": "^5.2.1",
    "nodemon": "^3.0.1",
    "sass": "^1.69.5",
    "typescript": "^4.6.2",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "axios": "^0.19.2",
    "custom-electron-titlebar": "^4.2.7",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.0",
    "electron-reloader": "^1.2.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.4",
    "fs-extra": "^11.1.1",
    "i18next": "^23.7.11",
    "minecraft-launcher-core": "^3.14.1",
    "minecraft-ping-server": "^1.0.3",
    "msmc": "^4.1.0",
    "onezip": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.19.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.8.0",
    "shell": "^0.5.1",
    "store": "^2.0.12",
    "sweetalert2": "^11.10.1"
  },
  "build": {
    "appId": "fr.dragonir.jflauncher",
    "productName": "JFLauncher",
    "artifactName": "${productName}-setup-${version}.${ext}",
    "compression": "maximum",
    "asar": true,
    "extends": null,
    "icon": "public/assets/logo.png",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "publisherName": "Team Project",
      "target": {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      }
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true
    },
    "linux": {
      "category": "games",
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ]
    }
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  }
}
